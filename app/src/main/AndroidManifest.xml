<?xml version="1.0" encoding="UTF-8"?>
<manifest package="com.ajarajeswarirsapplication.app" xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:largeHeap="true"
        android:name="com.ajarajeswarirsapplication.app.appcomponents.di.MyApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true" android:theme="@style/Theme.MyApp" android:usesCleartextTraffic="true">
        <activity
            android:name="com.ajarajeswarirsapplication.app.modules.seatingavailability.ui.SeatingAvailabilityActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.ajarajeswarirsapplication.app.modules.finalpage.ui.FinalPageActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.ajarajeswarirsapplication.app.modules.signin.ui.SignInActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.ajarajeswarirsapplication.app.modules.signup.ui.SignUpActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.ajarajeswarirsapplication.app.modules.faculty.ui.FacultyActivity" android:screenOrientation="portrait"/>
        <activity android:exported="true"
            android:name="com.ajarajeswarirsapplication.app.modules.welcomepage.ui.WelcomePageActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.ajarajeswarirsapplication.app.modules.seating.ui.SeatingActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.ajarajeswarirsapplication.app.modules.student.ui.StudentActivity" android:screenOrientation="portrait"/>
        <activity
            android:name="com.ajarajeswarirsapplication.app.modules.homepage.ui.HomePageActivity" android:screenOrientation="portrait"/>
    </application>
</manifest>
